MODULE m;
TYPE
    T = RECORD
    END;

    Derived = RECORD(T)
    END;

    RecordWithField = RECORD
        i: INTEGER;
    END;

    RecordWithFieldDerived = RECORD(T)
    END;

PROCEDURE T();
END;

PROCEDURE Derived();
END;

PROCEDURE RecordWithField();
END;

PROCEDURE RecordWithFieldDerived();
END;

PROCEDURE passAsArgument(o: T);
END;

BEGIN
    passAsArgument(T());
    r <- T();
    i <- RecordWithField().i;
END m.