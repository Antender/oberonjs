MODULE test;
VAR
    m: MAP OF INTEGER;

PROCEDURE ForEach();
BEGIN
    FOREACH v, k IN m DO
        ASSERT(v = 0);
        ASSERT(k # "");
    END;
END;

PROCEDURE put();
VAR
    s: STRING;
    a: ARRAY 3 OF CHAR;
BEGIN
    m["a"] := 1;
    m["abc"] := 2;
    m[s] := 3;
    m[a] := 4;
END;

END test.