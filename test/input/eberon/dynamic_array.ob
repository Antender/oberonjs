MODULE m;
TYPE
    T = RECORD
        a: ARRAY * OF INTEGER
    END;
VAR
    a: ARRAY * OF INTEGER;
    r: T;

PROCEDURE assignDynamicArrayFromStatic();
VAR 
    static: ARRAY 3 OF INTEGER;
    dynamic: ARRAY * OF INTEGER;
BEGIN
    dynamic := static;

    dynamic.add(3);
END assignDynamicArrayFromStatic;

END m.