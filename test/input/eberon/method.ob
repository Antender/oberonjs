MODULE m;
TYPE
    T = RECORD
		i: INTEGER
	END;
    D = RECORD(T) END;

PROCEDURE T.p(), NEW;
BEGIN
	SELF.i := 123;
END T.p;

PROCEDURE T.p2(i: INTEGER): INTEGER, NEW;
    RETURN i
END T.p2;

PROCEDURE D.p();
BEGIN
    SUPER()
END D.p;

PROCEDURE D.p2(i: INTEGER): INTEGER;
    RETURN SUPER(i)
END D.p2;

END m.