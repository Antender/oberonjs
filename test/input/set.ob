MODULE m;

CONST
	ci = 3;
	
	cs1 = {1} + {2};
	cs2 = {1, 2, 3} - {1, 4};
	cs3 = {1, 2, 3} * {1, 4};
	cs4 = {1, 2, 3} / {1, 4};
	cs5 = -{1};
VAR 
	s1, s2: SET;
	i1: INTEGER;
	b: BOOLEAN;

BEGIN
	s1 := {};
	s1 := {0, 2..5};
	s1 := {ci};
	s1 := {ci * 2};

	i1 := 3;
	s2 := {i1, i1 + 2, 10 - i1..15};
	s2 := {i1, 2};

	b := i1 IN s1;
	b := s1 <= s2;
	b := s1 >= s2;
	b := s1 = s2;
	b := s1 # s2;

	s1 := s1 + s2;
	s1 := s1 - s2;
	s1 := s1 * s2;
	s1 := s1 / s2;
	s1 := -s2;

	INCL(s2, 3);
	INCL(s1, ci * 2 + 3);
	INCL(s1, ci * 2 - i1 + 3);
	INCL(s1, ORD(b));

	EXCL(s2, 3);
	EXCL(s2, ORD(b))
END m.